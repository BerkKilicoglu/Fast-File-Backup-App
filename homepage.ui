}

#pushButton_srcDirDrive{
	background-color: #00AC47;
}

#btnSelectSrcDirectory::hover, #btnSelectLocation::hover, #pushButton_srcDirDrive::hover{
	background-color: #1F2A5B;
	color: white;
	font-size: 15px;
	font-weight: bold;
}

#pushButton_srcDirDrive::hover{
	background-color: #00832D;
}

#btnSelectSrcDirectory::pressed, #btnSelectLocation::pressed, #pushButton_srcDirDrive::pressed{
	background-color: #8BA0D6;
	font-size: 14px;
	font-weight: bold;
}

#pushButton_srcDirDrive::pressed{
	background-color: #BEEDB1;
}

#lineEdit_filetype, #txtBackupName, #txtBackupName_Drive, #lineEdit_filetype_Drive{
	border-radius: 10px;
	padding-left: 5px;
}

#btnBackupNow, #btnDriveBackup{
	font: 75 10pt &quot;Microsoft YaHei UI&quot;;
	font-weight: bold;
	color: #fff;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgb(61, 217, 245), stop:1 rgb(240, 53, 218));
	border: 3px solid  rgb(61, 217, 245);
	border-radius:60px;
}

#btnDriveBackup{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgb(255, 186, 0), stop:1 #ACD657);
	border: 3px solid  #BEDF7C;
}
#btnBackupNow::hover, #btnDriveBackup::hover{
	font: 75 10pt &quot;Microsoft YaHei UI&quot;;
	font-weight: bold;
	color: white;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #1F2A5B, stop:1 #8BA0D6);
	border: 3px solid #8BA0D6;
	border-radius:60px;
}

#btnDriveBackup::hover{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #f5ff00, stop:1 #7fe845);
	border: 3px solid #7fe845;

}

#btnBackupNow::pressed, #btnDriveBackup::pressed{
	font: 75 10pt &quot;Microsoft YaHei UI&quot;;
	font-weight: bold;
	color: white;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #415AAF, stop:1 #D4F1FF);
	border: 3px solid #D4F1FF;
	border-radius:60px;
}

#btnDriveBackup::pressed{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #fbfd00, stop:1 #517601);
	border: 3px solid #94c507;
}

