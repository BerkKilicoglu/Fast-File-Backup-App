	border-radius: 10px;
	border: 1.8px dotted #619EF1;
	background-color: #F5F8FC;
}

#pushButton_selectfile, #pushButton_blocation{
	background-color: #415AAF;
	border-radius: 25px;
	font-size: 14px;
	font-weight: bold;
	color: white;
}

#pushButton_selectfile::hover, #pushButton_blocation::hover{
	background-color: #1F2A5B;
	color: white;
	font-size: 15px;
	font-weight: bold;
}

#pushButton_selectfile::pressed, #pushButton_blocation::pressed{
	background-color: #8BA0D6;
	font-size: 14px;
	font-weight: bold;
}

#lineEdit_filetype{
	border-radius: 10px;
	padding-left: 5px;
}

#pushButton_process{
	font: 75 10pt &quot;Microsoft YaHei UI&quot;;
	font-weight: bold;
	color: #fff;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgb(61, 217, 245), stop:1 rgb(240, 53, 218));
	border: 3px solid  rgb(61, 217, 245);
	border-radius:60px;
}
#pushButton_process::hover{
	font: 75 10pt &quot;Microsoft YaHei UI&quot;;
	font-weight: bold;
	color: white;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #1F2A5B, stop:1 #8BA0D6);
	border: 3px solid #8BA0D6;
	border-radius:60px;
}
#pushButton_process::pressed{
	font: 75 10pt &quot;Microsoft YaHei UI&quot;;
	font-weight: bold;
	color: white;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #415AAF, stop:1 #D4F1FF);
	border: 3px solid #D4F1FF;
	border-radius:60px;
}

#pushButton_Storage{
	color: #415AAF;
	background-color: #FEFEFF;
	padding: 10px 5px;
	text-align: left;
	border-top-left-radius: 20px;
}

#pushButton_2_Dashboard{
	padding: 10px 5px;
	text-align: left;
}

#pushButton_gitberk, #pushButton_gitemre{
	background-color: #415AAF;
	color: white;
}
